{
    "name": "strixnl/advanced-cms-extensions",
    "description": "Plugin for Advanced CMS Elements",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Joris"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jorisstrix/StrixAdvancedCMSExtensions"
        }
    ],
    "require": {
        "shopware/core": "~6.7.0"
    },
    "extra": {
        "shopware-plugin-class": "StrixAdvancedCMSExtensions\\StrixAdvancedCMSExtensions",
        "label": {
            "de-DE": "Strix Erweiterte CMS-Erweiterungen",
            "en-GB": "Strix Advanced CMS Extensions",
            "nl-NL": "Strix Geavanceerde CMS-extensies"
        }
    },
    "autoload": {
        "psr-4": {
            "StrixAdvancedCMSExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StrixAdvancedCMSExtensions\\Tests\\": "tests/"
        }
    }
}