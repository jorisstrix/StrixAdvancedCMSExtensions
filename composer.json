{
    "name": "strix/advanced-cms-extensions",
    "description": "Plugin for Advanced CMS Elements",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris"
        }
    ],
    "require": {
        "shopware/core": "~6.7.0"
    },
    "extra": {
        "shopware-plugin-class": "StrixAdvancedCMSExtensions\\StrixAdvancedCMSExtensions",
        "label": {
            "de-DE": "Strix Erweiterte CMS-Erweiterungen",
            "en-GB": "Strix Advanced CMS Extensions",
